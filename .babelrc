{
  "presets": [
    "babel-preset-react-native-stage-0/decorator-support",
    "react-native"
  ],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "react-native-sass-classname",
      {
        "extension": ["css", "scss", "sass"],
        "prefixExtension": "native"
      }
    ]
  ],
  "ignore": "node_modules"
}
